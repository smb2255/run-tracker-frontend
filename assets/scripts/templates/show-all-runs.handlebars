 {{#each runs as |run|}}
<section id="show-all-runs">
  <div data-id="{{run.id}}" class="allRuns">
    <div class="center">
    <strong>ID: {{run.id}}</strong>
    <strong>Date: {{run.date}}</strong>
    <strong>Distance: {{run.distance}}mi</strong>
    <strong>Time: {{run.time}}</strong>
    <br>
  </div>
  </section>
{{/each}}
